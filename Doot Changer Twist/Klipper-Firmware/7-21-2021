[gcode_macro PARK_extruder]
gcode:
    SAVE_GCODE_STATE NAME=park0
    
    M204 S2000    

    G1 Y250 x10.5 f30000
    G1 Y280 f6000

    FORCE_UNLOCK
    SAVE_VARIABLE VARIABLE=currenttool VALUE='"none"'

    {% if printer.extruder.temperature >= 170%}
        G92 E0
        G1 E-1 F2400
        G92 E0
    {% endif %}

    G1 Y250 f30000

    RESTORE_GCODE_STATE NAME=park0
    SET_INPUT_SHAPER SHAPER_FREQ_X=0 SHAPER_FREQ_Y=0
    SET_GCODE_OFFSET Z_ADJUST=0.2  move=1
[gcode_macro PICK_extruder]
gcode:

    PARK_CURRENT
    ACTIVATE_EXTRUDER EXTRUDER=extruder
    SAVE_GCODE_STATE NAME=park0

    M204 S2000 

    {% if printer.extruder.temperature >= 170%}
        G92 E0
        G1 Y250 x10.5 E15 F6000
        G92 E0
        G1 E-1 F3000
        G92 E0
    {% else %}
        G1 Y250 x10.5 f18000
    {% endif %}

#    G1 Y250 x10.5 f18000
    G1 Y286 f3600

    FORCE_LOCK
    SAVE_VARIABLE VARIABLE=currenttool VALUE='"extruder"'

    G1 Y230 F6000
    force_lock

    {% if printer.extruder.temperature >= 170%}
        G1 Y270 F6000
        G1 Y230 F6000
        G1 Y270 F6000
        G1 Y230 F6000
        G1 Y270 F6000
        G1 Y230 F6000
    {% endif %}

    G1 Y220 f10000


    RESTORE_GCODE_STATE NAME=park0
    SET_GCODE_OFFSET Z_ADJUST=-0.2  move=1 speed=100
    SET_INPUT_SHAPER SHAPER_FREQ_X=84.6 SHAPER_TYPE_X=3hump_ei SHAPER_FREQ_Y=50.2 SHAPER_TYPE_Y=mzv

