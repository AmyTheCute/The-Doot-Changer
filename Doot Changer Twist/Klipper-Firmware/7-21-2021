[gcode_macro PICK_mill]
gcode:
    PARK_CURRENT
    SAVE_GCODE_STATE NAME=park0

    M204 S8000 

    G91
    g1 z5 f20000
    g90
    G1 Y250 X210.9 f30000
    G1 Y279.5 f6000

    FORCE_LOCK
    SAVE_VARIABLE VARIABLE=currenttool VALUE='"mill"'

    G1 Y250 f30000

    MANUAL_STEPPER STEPPER=tool_lock set_position=0
    MANUAL_STEPPER STEPPER=tool_lock Move=300 SPEED=600
    MANUAL_STEPPER STEPPER=tool_lock enable=0

    G91
    g1 z-5 f30000
    g90

    RESTORE_GCODE_STATE NAME=park0
    SET_INPUT_SHAPER SHAPER_FREQ_X=0 SHAPER_FREQ_Y=0
    SET_GCODE_OFFSET Z_ADJUST=30.35 X_ADJUST=-0.2 Y_ADJUST=16.5 move=1 MOVE_SPEED=30000
    M204 s200
    SET_PIN PIN=millpin VALUE=1

[gcode_macro PARK_mill]
gcode:
    SET_PIN PIN=millpin VALUE=0
    SET_GCODE_OFFSET X_ADJUST=0.2 Y_ADJUST=-16.5 move=1 MOVE_SPEED=30000

    SAVE_GCODE_STATE NAME=park0

    M204 S8000 

    G1 Y250 x210.9 f30000
    G1 Y279 F3600

    FORCE_UNLOCK
    SAVE_VARIABLE VARIABLE=currenttool VALUE='"none"'

    G1 Y250 f30000

    RESTORE_GCODE_STATE NAME=park0

    SET_INPUT_SHAPER SHAPER_FREQ_X=0 SHAPER_FREQ_Y=0
    SET_GCODE_OFFSET Z_ADJUST=-30.35


# Activate the primary extruder
[gcode_macro T0]
gcode:
    {% if not printer.toolhead.homed_axes %}
        M118 PP > Error: Not homed! {printer.toolhead.homed_axes}
    {% else %}
        {% set svv = printer.save_variables.variables %}
        {% if not (svv.currenttool == "extruder") %}
            {% if not (svv.currenttool == "none") %}
                PARK_{svv.currenttool}
            {% endif %}
            PICK_EXTRUDER
        {% endif %}
    {% endif %}

