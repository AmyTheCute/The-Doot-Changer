[gcode_macro FORCE_LOCK]
gcode:
     SET_PIN PIN=STDBY VALUE=1
     SET_PIN PIN=LOCK_IN1 VALUE=1
     SET_PIN PIN=LOCK_IN2 VALUE=0
     SET_PIN PIN=LOCK_PWM VALUE=1.0
     G4 P300
     SET_PIN PIN=LOCK_PWM VALUE=0.8
     G4 P50
     SET_PIN PIN=LOCK_PWM VALUE=0.5
     G4 P50
     SET_PIN PIN=LOCK_PWM VALUE=0.3
     G4 P50
     SET_PIN PIN=LOCK_PWM VALUE=0.1
     G4 P50
     SET_PIN PIN=LOCK_PWM VALUE=0.0
     SET_PIN PIN=STDBY VALUE=0
     SET_PIN PIN=LOCK_IN2 VALUE=1

